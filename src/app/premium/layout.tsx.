import React from 'react'; // <--- ¡ASEGURAMOS LA IMPORTACIÓN DE REACT!

// Este Layout es una envoltura para la ruta '/premium'.
// Usamos una etiqueta <section> para asegurar que Next.js lo reconozca
// como un componente estructural válido que hereda del layout raíz.
export default function PremiumLayout({
  children,
}: {
  children: React.ReactNode;
}) {
  return (
    <section>
      {children}
    </section>
  );
}
